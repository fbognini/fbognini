<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>fbognini.Infrastructure.Identity</PackageId>
    <TargetFrameworks>net6.0;netcoreapp3.1</TargetFrameworks>
    <Version>6.2.0</Version>
    <Authors>Francesco Bognini</Authors>
    <Company>Francesco Bognini</Company>
    <PackageProjectUrl>https://github.com/fbognini/fbognini</PackageProjectUrl>
    <RepositoryUrl>https://github.com/fbognini/fbognini</RepositoryUrl>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Identity.Stores" Version="6.0.5" />
  </ItemGroup>
  
  <PropertyGroup Condition=" '$(TargetFramework)' == 'net6.0'">
    <DefineConstants>NET6</DefineConstants>
  </PropertyGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\fbognini.Core.Identity\fbognini.Core.Identity.csproj" />
    <ProjectReference Include="..\fbognini.Infrastructure\fbognini.Infrastructure.csproj" />
  </ItemGroup>

</Project>
